            <xml-property name="queryText"><![CDATA[SELECT
    u_holder.full_name AS applicant,  -- Заявитель
    i.name AS item_name,             -- Название вещи
    u_owner.full_name AS owner,       -- Владелец вещи (сам пользователь)
    r.start_date,                     -- Дата начала бронирования
    r.end_date,                       -- Дата окончания бронирования
    rs.description AS status_desc     -- Описание статуса
FROM request r
         JOIN users u_holder ON r.holder = u_holder.passport_num  -- Заявитель
         JOIN item i ON r.item = i.item_id                         -- Вещь
         JOIN users u_owner ON i.owner = u_owner.passport_num      -- Владелец вещи
         JOIN request_status rs ON r.status = rs.status_id         -- Статус заявки
WHERE i.owner = ?  -- Фильтр по владельцу вещи
ORDER BY r.start_date DESC;]]></xml-property>
